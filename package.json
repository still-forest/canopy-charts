{
  "name": "@still-forest/canopy-charts",
  "version": "0.2.0",
  "author": "John Szymanowski",
  "license": "MIT",
  "description": "React charts, built with VisX and Canopy",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "private": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/still-forest/canopy-charts.git"
  },
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "dev": "vite",
    "format": "biome format --write .",
    "lint": "biome check .",
    "lint:ci": "biome ci .",
    "lint:fix": "biome check --write --unsafe .",
    "prepublishOnly": "pnpm build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:build:docs": "storybook build --docs",
    "storybook:docs": "storybook dev --docs",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "release": "release-it",
    "release:dry-run": "release-it --dry-run"
  },
  "dependencies": {
    "@radix-ui/react-separator": "^1.1.4"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.0-beta.2",
    "@jszymanowski/biomejs-config": "1.2.0",
    "@storybook/addon-essentials": "8.6.12",
    "@storybook/addon-interactions": "8.6.12",
    "@storybook/addon-themes": "8.6.12",
    "@storybook/builder-vite": "8.6.12",
    "@storybook/react": "8.6.12",
    "@storybook/react-vite": "8.6.12",
    "@storybook/test": "8.6.12",
    "@tailwindcss/vite": "4.1.5",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@types/lodash": "^4.17.16",
    "@types/node": "22.15.14",
    "@types/react": "19.1.3",
    "@types/react-dom": "19.1.3",
    "@vitejs/plugin-react": "4.4.1",
    "@vitest/browser": "3.1.3",
    "clsx": "2.1.1",
    "jsdom": "26.1.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "release-it": "^19.0.2",
    "storybook": "8.6.12",
    "tailwind-merge": "3.2.0",
    "tailwindcss": "4.1.5",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vite-plugin-dts": "4.5.3",
    "vite-plugin-lib-inject-css": "2.2.2",
    "vitest": "3.1.3"
  },
  "peerDependencies": {
    "@jszymanowski/proper-date.js": "^0.6.0",
    "@still-forest/canopy": "^0.10.0",
    "@visx/axis": "^3.12.0",
