{
  "name": "@still-forest/canopy-charts",
  "version": "0.4.1",
  "author": "John Szymanowski",
  "license": "MIT",
  "description": "React charts, built with VisX and Canopy",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "private": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/still-forest/canopy-charts.git"
  },
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "dev": "vite",
    "format": "biome format --write .",
    "lint": "biome check .",
    "lint:ci": "biome ci .",
    "lint:fix": "biome check --write --unsafe .",
    "prepublishOnly": "pnpm build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:build:docs": "storybook build --docs",
    "storybook:docs": "storybook dev --docs",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "release": "release-it",
    "release:dry-run": "release-it --dry-run"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@still-forest/biomejs-config": "2.0.1",
    "@storybook/addon-docs": "9.0.16",
    "@storybook/addon-themes": "9.0.16",
    "@storybook/react-vite": "9.0.16",
    "@tailwindcss/vite": "4.1.12",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@types/lodash": "4.17.20",
    "@types/node": "22.15.33",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "@vitejs/plugin-react": "5.0.2",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "jsdom": "26.1.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "release-it": "19.0.4",
    "storybook": "9.0.16",
    "tailwindcss": "4.1.12",
    "typescript": "5.8.3",
    "vite": "7.0.4",
    "vite-plugin-dts": "4.5.4",
    "vite-plugin-lib-inject-css": "2.2.2",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "@still-forest/canopy": "^0.21.0",
    "@still-forest/proper-date.js": "^0.8.1",
    "@visx/axis": "^3.12.0",
    "@visx/brush": "^3.12.0",
    "@visx/curve": "^3.12.0",
    "@visx/event": "^3.12.0",
    "@visx/gradient": "^3.12.0",
    "@visx/grid": "^3.12.0",
    "@visx/group": "^3.12.0",
    "@visx/responsive": "^3.12.0",
    "@visx/scale": "^3.12.0",
    "@visx/shape": "^3.12.0",
    "@visx/tooltip": "^3.12.0",
    "@visx/vendor": "^3.12.0",
    "lodash": "^4.17.21",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "tailwindcss": "^4.0.0"
  },
  "packageManager": "pnpm@10.13.1"
}
